                      <m:mo stretchy="false">&lt;</m:mo>
                      <m:mi>w</m:mi>
                    </m:mrow>
                    <m:mo stretchy="false">(</m:mo>
                    <m:mover accent="true">
                      <m:mi>y</m:mi>
                      <m:mo>¯</m:mo>
