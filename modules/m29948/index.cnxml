          <section id="id-307736966589">
            <title>Giai đoạn 2 : Kiểm tra tính tối ưu</title>
            <para id="id4916852">Đây là phương án tối ưu</para>
            <table id="id4916857" summary="">
              <tgroup cols="4">
                <colspec colnum="1" colname="c1"/>
                <colspec colnum="2" colname="c2"/>
                <colspec colnum="3" colname="c3"/>
                <colspec colnum="4" colname="c4"/>
                <tbody>
                  <row>
                    <entry/>
                    <entry>80</entry>
                    <entry>20</entry>
                    <entry>60</entry>
                  </row>
                  <row>
                    <entry>50</entry>
